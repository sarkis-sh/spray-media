{
  "name": "sarkis-sh/spray-media",
  "description": "Secure uploads, signed URLs, and flexible media delivery for Laravel.",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "media", "upload", "download", "storage", "file-server", "hmac", "signed-urls", "secure", "protected", "api", "files", "attachments", "validation", "translations", "package"],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "orchestra/testbench": "^8.0 || ^9.0 || ^10.0"
  },
  "autoload": {
    "psr-4": {
      "SprayMedia\\": "src/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SprayMedia\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SprayMedia\\Providers\\SprayMediaServiceProvider"
      ],
      "aliases": {
        "SprayMedia": "SprayMedia\\Facades\\SprayMedia"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}